<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/prent_frag"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="id.rentist.mitrarentist.fragment.HistoryAcceptFragment">

    <android.support.v7.widget.RecyclerView
        android:id="@+id/haccept_recyclerViewFrag"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="@dimen/xmin_padding">
    </android.support.v7.widget.RecyclerView>

    <com.github.ybq.android.spinkit.SpinKitView
        android:id="@+id/progressBar"
        style="@style/SpinKitView.FadingCircle"
        android:layout_width="match_parent"
        android:layout_height="@dimen/btn_lmedium_height"
        android:layout_marginTop="@dimen/breadcrumb_height"
        android:background="@android:color/transparent"
        android:padding="@dimen/min_padding"
        android:visibility="gone"
        app:SpinKit_Color="@color/colorWhite" />

</LinearLayout>